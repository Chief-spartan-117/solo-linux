    <div class="contact" id="3">
      <div class="clip">
        <p class="tag">Want to contact me, Leave a message</p>
        <div class="form">
          <form id="smsg">
            <input
              type="text"
              name="name"
              placeholder="Name"
              class="all"
              id="name"
              autocomplete="off"
              required
            />
            <br />
            <input
              type="email"
              name="email"
              placeholder="E-mail"
              class="all"
              id="email"
              autocomplete="off"
              required
            />
            <br />
            <textarea
              name="msg"
              placeholder="Your message"
              class="all"
              rows="10"
              cols="45"
              id="msg"
              required
            ></textarea>
            <br />
            <input
              type="submit"
              name="submit"
              value="Send"
              class="send"
              id="send"
            />
          </form>
          <div class="sent"></div>
        </div>
      </div>

